<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="900.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monopoly.view.MenuController">
   <children>
      <ImageView fitHeight="900.0" fitWidth="1601.0" layoutX="-401.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/Monopoly_Menu.png" />
         </image>
      </ImageView>
      <Button layoutX="728.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleStart" prefHeight="78.0" prefWidth="282.0" stylesheets="@Theme.css" text="Start Game" />
      <Button layoutX="728.0" layoutY="425.0" mnemonicParsing="false" onAction="#handleHowToPlay" prefHeight="78.0" prefWidth="282.0" stylesheets="@Theme.css" text="How to Play" />
      <Button layoutX="728.0" layoutY="559.0" mnemonicParsing="false" onAction="#handleScoreboard" prefHeight="78.0" prefWidth="282.0" stylesheets="@Theme.css" text="Scoreboard" />
      <Button layoutX="728.0" layoutY="690.0" mnemonicParsing="false" onAction="#handleExit" prefHeight="78.0" prefWidth="282.0" stylesheets="@Theme.css" text="Exit Game" />
   </children>
</AnchorPane>
